datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model UserProfile {
  country     String?
  createdAt   DateTime  @default(now())
  dateOfBirth DateTime?
  firstName   String?
  gender      String?
  id          String    @id @default(cuid())
  lastName    String?
  profileId   String?
  updatedAt   DateTime  @updatedAt
  userId      String?
}
